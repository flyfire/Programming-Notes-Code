<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.ztiany.recyclerview"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ztiany.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name="com.ztiany.wrapcontent.WrapContentActivity"
            android:label="@string/title_activity_wrap_content"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name="com.ztiany.snap.SnapHelperActivity"/>
        <activity android:name="com.ztiany.itemtouch.ItemTouchActivity"/>
        <activity android:name="com.ztiany.itemtouchhelperextension.ItemTouchHelperExtensionActivity"/>
        <activity android:name="com.ztiany.diffutils.DiffUtilsActivity"/>


    </application>

</manifest>